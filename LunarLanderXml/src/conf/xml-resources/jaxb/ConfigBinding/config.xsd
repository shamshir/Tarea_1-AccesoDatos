  <xs:element name="configuraciones">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="configuracion" maxOccurs="unbounded" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:string" name="modoDificil"/>
              <xs:element type="xs:string" name="colorNave"/>
            </xs:sequence>
            <xs:attribute type="xs:string" name="nombre" use="optional"/>
